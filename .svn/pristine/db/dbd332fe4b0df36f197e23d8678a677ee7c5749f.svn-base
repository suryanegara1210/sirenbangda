#teks_page_content{float:left; width:800px; text-align:justify; margin:10px 0 10px 0; font-size:13px; line-height:19px;
	text-shadow: 0 1px white;}
	
.news_rel{width:365px; float:left; padding:10px 10px; border-bottom:1px dashed #ccc; margin-bottom:10px; text-align:justify;
	line-height:17px; text-shadow: 0 1px white;}	
.news_rel .judul{display:block; width:300px; font-weight:bold; padding:0px 14px 0px 0; float:left; border-right: 1px solid #ccc;
margin-bottom:5px}
.news_rel .time{display:block; width:50px; font-style:italic; padding-bottom:5px; float:right; text-align:right}
.news_rel .isi{display:block; float:left}
.news_rel .isi a{color:#333}

.news_rel_old{width:780px; float:left; padding:10px 10px; border-bottom:1px dashed #ccc; margin-bottom:10px; text-align:justify;
	line-height:17px; text-shadow: 0 1px white;}	
.news_rel_old .judul{display:block; width:715px; font-weight:bold; padding:0px 14px 0px 0; float:left; border-right: 1px solid #ccc;
margin-bottom:5px}
.news_rel_old .time{display:block; width:50px; font-style:italic; padding-bottom:5px; float:right; text-align:right}
.news_rel_old .isi{display:block; float:left}
.news_rel_old .isi a{color:#333}

.more{text-align:center; width:365px; float:left; padding:5px 10px; line-height:17px; text-shadow: 0 1px #2288a5; background:#2f96b4;
margin-bottom:10px; color:#fff;
	border-radius:2px;
	-moz-border-radius:2px;
	-webkit-border-radius:2px;}	

.ticker a:hover .news_rel{background:#eeeded;
	transition: all 0.07s;
	-webkit-transition: all 0.07s;  
    -moz-transition: all 0.07s; }
.ticker a:hover .more{background:#2288a5}

textarea.homebar{width:385px; height:180px; border:none; border-bottom:1px solid #cdcdcd; margin-bottom:0; padding:10px 10px}
input.btn.homebar{width:100px; color:#fff; border-radius: 2px 2px 0 0; margin:5px; float:right}

/*FORM*/

.widget_content form{width:800px; float:left; display:table}
.field_wrap{float:left; height:auto; display:table; padding-bottom:5px;}
.field_wrap.select{height:38px}
.field_wrap.full{width:800px}
.field_wrap.dua{width:385px} .field_wrap.dua.kiri{margin-right:30px}
.field_wrap.full input.form_field{width:658px}
.field_wrap.judul input.form_field{width:788px}
.field_wrap.dua input.form_field{width:243px}
.field_wrap.error_message{width:360px}
.field_wrap.error_message label{float: left; width: 360px; color: red;}
.field_wrap select{width:253px; border-radius: 3px; font-size:13px}
.field_wrap textarea{width:658px; height:100px; border-radius: 3px; font-size:13px}
.field_wrap label{float:left; width:120px; font-size:13px; padding-top:7px; padding-right:10px}

.field_wrap.batas{height:10px}
.field_wrap .btn{float:right}

.field_wrap.judulform{border-bottom:1px solid #ccc; margin-bottom:10px; font-weight:bold}
.field_wrap.judulform label{width:400px}

#foto_form{width:130px; max-height:181px; min-height:150px; float:right; padding:4px; overflow:hidden; border:1px solid #ccc;
border-radius:2px}
.loader_form{background:#fff url(../images/template/progress.gif) center no-repeat ; width:130px; height:150px; float:right;}
#wrap_foto_form{max-height:173px; width:130px; float:left; overflow:hidden}

/*POP UP*/

#wrap_pop, #wrap_pop_2 {width:100%; height:100%; position:fixed; float:left; background:rgba(255,255,255,0.5); z-index:100; top:0; left:0;
text-align:center; overflow:auto}
#image_pop{height:auto; width:515px; background:#fafafa; margin:auto; margin-top:20%; border:10px solid #fff; padding-bottom:30px;
-webkit-box-shadow: 0px 1px 5px 0px #999;
-moz-box-shadow: 0px 1px 5px 0px #999;
box-shadow: 0px 1px 5px 0px #999;}

 .other a{cursor:pointer}
 #closepop {margin-left:445px}
 #judul_pop{width:515px; float:left; margin:25px 0; color:#777}
 #loading{background:url(../images/template/progress.gif) center bottom no-repeat ; width:110px; height:80px; display:block; margin:0 auto; }

#wrap_tombol{width:auto; height:auto; display:table; margin:0 auto}
.tombol{width: auto; padding:10px 20px 4px;
height: 24px;
font-size: 15px; margin:5px;
float:left}

/*tip*/

.tip_info{float:right; background:none; height:auto; width:auto; }
.badge-info.tip{float:right; position:static; margin-top:8px; cursor:pointer}
.tip_caption{float:right; width:255px; height:auto; background:#e5e5e5; font-size:12px; font-weight:normal; margin-right:5px;
border-radius:2px;padding:2px 5px; position:absolute; margin-left:-250px; margin-top:20px; display:none}
.tip_info:hover .tip_caption{display:block; margin-top:30px;
transition: all 2s; -webkit-transition: all 2s;  moz-transition: all 2s; }
 
/*info riwayat*/
#info{position:absolute; float:left; margin: 10px 0 0 15px; height:auto; width: 800px;}
#info span{ display:block; padding-bottom:7px; width:790px; float:left}
#info span p.kiri{width:50px; display:inline; float:left}
#info span p{display:inline; float:left}

