.tree-wrapper {
	border: solid #CDCDCD 0px;
	border-right-width: 1px;
	border-bottom-width: 1px;
}
.tree-wrapper ul.ul-first {
	margin-left: 0;
}
.tree-wrapper ul {
	margin-left: 21px;
	background: #EEEEEE;
	list-style-position:inside;
}
.tree-wrapper ul li {
	border: solid #CDCDCD 0px;
	border-left-width: 1px;
	border-top-width: 1px;
	line-height: 25px;
	padding-left: 6px;
}
.tree-wrapper ul.ul-last {
	list-style:none;
	background: #FAFAFA;
}
.tree-wrapper input[type="text"] {
	margin-top:1px;
	margin-bottom:1px;
	height: 14px;
}
.tree-wrapper span.input-1 {
	float: right;
    text-align: center;
    width: 116px;
}
