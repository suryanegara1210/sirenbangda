/*==========================================login==================================================================*/
#wrap_login{
	margin:15% auto 0; display:table;
	width:900px; height:295px;
	border-radius:2px;
	-moz-border-radius:2px;
	-webkit-border-radius:2px;
	background:#eceded;	
}

.login {
	padding:20px 20px 15px;
	width:250px; height:260px;
	float:left; border-right:1px solid #d7d7d7;
	overflow:hidden
}
.login img {
	width:100px;
	left:32%;
	margin:15px 0 20px;
}

.link.kiri a{
    width: 120px;
    float: left;
	text-align: left;
}

.link.kanan a{
    width: 120px;
    float: right;
    text-align: right;
}

.login input{width:240px}
.login input.btn{width:250px; color:#fff; border-radius: 2px 2px 0 0;}

#form_result{width:250px; height:17px; background:#777; float:left; margin-top:-1px; border-radius: 1px; text-align:center; 
font-size:12px; padding-top:3px; text-shadow:0 1px 1px rgba(0,0,0,0.2); color:#fff;
border-bottom:1px solid;  border-bottom-color:#566;}

.photo_frame{
	width:598px; height:285px; background:url(../images/template/login_cover.jpg) center no-repeat; padding:5px;
	float:left; border-left:1px solid #f0f0f0;
}

#caption_wrap{width:61px; height:35px; position:absolute; background:none; margin-left:210px; overflow:hidden}
#caption_wrap.a2{margin-left:328px;}
#caption_login{width:61px; height:35px; position:relative; background:url(../images/template/lancip.png);}
#caption_login.a2{background:url(../images/template/lancip2.png);left:1000px}
#caption_login.a1{left:1000px}

#photo_wrap{width:598px; height:285px; float:left; position:absolute; overflow:hidden}
#caption_login2{width:414px; height:35px; position:relative; margin-top:15px; left:594px;}
	#cap_kiri{width:24px; height:35px; background:url(../images/template/lancip2.png) no-repeat left; float:left}
	#cap_kanan{height:35px; width:390px; background:rgba(0,0,0,0.38); float:left}
	#cap_kanan p{color:#fff; padding:6px 0 0 10px; float:left; font-size:20px; text-shadow:0 1px 1px rgba(0,0,0,0.5);}


#foot_login, #foot{
	margin:10px auto 8%; display:table;
	width:900px; height:auto;
