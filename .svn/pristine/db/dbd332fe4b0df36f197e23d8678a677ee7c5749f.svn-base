}
#sidebar ul.nav li a.link {
	display:block;
	text-decoration:none;
	color:#555;
	padding:0px 0;
	background-position:2px 4px;
	background-repeat:no-repeat;
	padding:0 0 1px;
}
.sidebar_navigation ul.nav li.active a.link{
	color:#454545;
}
#sidebar ul.nav li a.link:hover {
	text-decoration:none;
	transition: all 0.07s;
	-webkit-transition: all 0.07s;  
    -moz-transition: all 0.07s; 
	text-shadow:none;
	color:#454545;
	background-position:2px 4px;
	padding:1px 0 0 ;
}

#sidebar ul.nav li:hover ul.subnav,#sidebar ul.nav li:hover ul.subnav li:hover ul.subsubnav{display:block}

#sidebar ul.nav li ul.subnav{
	-moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #ECEDED;
    border-color: -moz-use-text-color #CDCDCD #CDCDCD;
    border-image: none;
    border-right: 1px solid #CDCDCD;
    border-style: none solid solid;
    border-width: medium 1px 1px;
    box-shadow: 0 0 5px #898989;
    display: none;
    float: left;
    margin-left: 134px;
    margin-top: -35px;
    padding: 0;
    position: absolute;
	z-index: 999;
}

#sidebar ul.nav li ul.subnav li{
    background: none repeat scroll 0 0 transparent;
    border-top: 1px solid #CDCDCD;
    list-style: none outside none;
    padding: 0;
    width: 100%;
	margin:0;
	
	border-top-right-radius:0;
	border-top-left-radius:0;
	border-bottom-right-radius:0;
	border-bottom-left-radius:0;
}

#sidebar ul.nav li ul.subnav li a:hover{
    background: #DBDBDB;
}

#sidebar ul.nav li ul.subnav li a{
	cursor: pointer;
    display: block;
    line-height: 32px;
    min-height: 0;
    width: 150px;
	padding: 0 10px;
}

#sidebar ul.nav li ul li a span {
    display: block;
    float: right;
}

#sidebar ul.subnav li ul.subsubnav{margin-top:-1px; margin-left:170px;}

#sidebar ul.subnav li ul.subsubnav li a{ width: 330px;}

#sidebar ul.nav li ul.subnav li ul.subsubnav{
	-moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #ECEDED;
    border-color: -moz-use-text-color #CDCDCD #CDCDCD;
    border-image: none;
    border-right: 1px solid #CDCDCD;
    border-style: none solid solid;
    border-width: medium 1px 1px;
    box-shadow: 2px 0 4px #898989;
    display: none;
    float: left;
    margin-left: 170px;
    margin-top: -33px;
    padding: 0;
    position: absolute;
	z-index: 999;
}

