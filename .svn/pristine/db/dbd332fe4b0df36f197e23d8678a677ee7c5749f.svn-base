	border: solid #CDCDCD 0px;
	border-left-width: 1px;
} */

.tabel_dupak {
	border: solid #CDCDCD 1px;
	border-radius: 2px 2px 0 0;
	background: #EEEEEE;
	width: 800px;
}
.tabel_dupak thead {
	background: linear-gradient(to bottom, #F8F8F8 0px, #E8E8E8 100%) repeat scroll 0 0 transparent;	
}
.tabel_dupak tr td {
	padding: 3px 8px;
	border: solid #CDCDCD 0px;
	border-right-width: 1px;
}
.tabel_dupak tr td.right {
	padding: 3px 8px;
	border-right-width: 0px;
}

.tabel_dupak tr td.nilai {
	width: 90px;
	text-align:center
}
.tabel_dupak tr td.opsi {
	width: 40px;
	text-align:center
}
.tree-wrapper td.nilai {
	width: 88px;
	text-align:center
}
.tree-wrapper td.opsi {
	width: 48px;
	text-align:center;
}
.tree-wrapper td.opsi span span {
	background: url("../images/template/tmp_edit.png") no-repeat scroll 2px 1px / 20px auto transparent;
    border-radius: 3px 3px 3px 3px;
    cursor: pointer;
    padding: 2px 11px;
}
.tree-wrapper td.opsi span {
	border-radius: 3px 3px 3px 3px;
	padding: 2px 0;
}
.tree-wrapper td.opsi span:hover {
	background-color:#DEDEDE;
}

ul.pilihan-opsi {
	-moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: #CDCDCD;
    border-image: none;
    border-style: solid;
    border-width: 0 1px 1px 0;
    list-style: none outside none;
    margin-left: -12px;
    position: absolute;
	box-shadow: 0 0 4px 0 #787878;
	display:none;
}
ul.pilihan-opsi li {
	width: 70px;
	cursor:pointer;
}
ul.pilihan-opsi li:hover {
	background-color:#DEDEDE;
}

.tambah-data {
	background: url("../images/template/content.png") no-repeat scroll 0 0 / 17px auto transparent;
    color: #777777;
    cursor: pointer;
    padding-left: 19px;
}

