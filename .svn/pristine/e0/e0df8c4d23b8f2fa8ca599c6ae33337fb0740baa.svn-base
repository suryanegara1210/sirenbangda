    <script type="text/javascript">
    //load file
		var reader = new FileReader();
		function readText(that){

			if(that.files && that.files[0]){
				var reader = new FileReader();
				reader.onload = function (e) {  
					var output=e.target.result;
					//alert (output);
					//process text to show only lines with "@":				
					//output=output.split("\n").filter(/./.test, /\@/).join("\n");
					output=output.split("\n");
					var hasil='';
					var has = new Array();
					var hasCount=0;
						output.forEach(function(out) {
						  var batas=document.getElementById('batas').value;
							if(hasCount>=batas-1){
							    	baris=out.split("<?php echo constant::CSV_SEPARATOR ; ?>");
								var has2Count=0;
								var has2 = new Array();
								baris.forEach(function(kolom) {						
									has2[has2Count]=kolom;
									//hasil=hasil+kolom+"<--->";
									has2Count++;
								});
								//hasil=hasil+"<br>";
								has[hasCount-batas+1]=has2;
							}
							hasCount++;
						});					    
                        //masukan ke row
                        masukkan(has);
						//alert(has[3][0]);
						//document.getElementById('main').innerHTML=hasil;
				};//end onload()
				
				reader.readAsText(that.files[0]);
			}//end if html5 filelist support
			
		} 
        //input ke tabel
        function masukkan(ambilan){
            if (ambilan.length>0){
                for(var i=0;i<ambilan.length;i++){
                    if (i==0){
                        form_barang = document.forms['form_barang'];
                        form_barang.elements["nama[0]"].value=ambilan[i][0];
                        form_barang.elements["jabatan[0]"].value=ambilan[i][1];
                        form_barang.elements["golongan[0]"].value=ambilan[i][2];
                        form_barang.elements["bruto[0]"].value=ambilan[i][3];
						form_barang.elements["grand_total"].value=ambilan[i][3];

						
                    }else{
                        addRow('nama_honor',ambilan[i]);
                    }
                }
            }  
        }
    </script>
