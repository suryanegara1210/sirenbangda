html, div, map, dt, isindex, form, header, aside, section, section, article, footer {  
    display: block;  
}

body{
background-color:#FFFFFF;      
background-image:url(../images/tes.jpg); 
background-attachment:fixed;
background-position:center bottom;
font-family:Arial, Helvetica, sans-serif;
font-size:12px;
padding:0;
margin:0;
z-index: 1;
}

#box-login{
	margin-top:65%;
width:380px;
height:50%;
border:2px solid #666666;
-moz-border-radius:10px;
-khtml-border-radius: 10px;
-webkit-border-radius: 10px;
border-radius:10px;   
background-repeat:repeat-x;  
background-position:bottom;
z-index: -2;
}
#bal{
margin:-250px 490px auto;
padding-top: -100px;
z-index: 10;
}
@-moz-keyframes rotate_unud {
    from {
        -moz-transform: rotate(0deg);        
    }
    to {
        -moz-transform: rotate(360deg);
        }
}

@-webkit-keyframes rotate_unud {
    from {
        -webkit-transform: rotate(0deg);        
    }
    to {
        -webkit-transform: rotate(360deg);
    }
}
#bgc{ background: url(../images/putar.png) center no-repeat;  position: absolute;  width: 110px; height: 100px;
    margin: 0 130px; 

    -webkit-animation-name: rotate_unud;
    -webkit-animation-duration: 10s;
    -webkit-animation-iteration-count: infinite;
       -webkit-animation-timing-function: linear;
      -moz-animation-name: rotate_unud;
    -moz-animation-duration: 10s;
    -moz-animation-iteration-count: infinite;
       -moz-animation-timing-function: linear;
}
h9 {
    font: normal 120% Georgia;
    color: #265180;     
}



