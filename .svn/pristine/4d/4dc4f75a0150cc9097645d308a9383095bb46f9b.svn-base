    function data_terceklist2(){
            if($this->auth->is_logged_in() == true){       
            $tgl_skrg = date("Y-m-d");
            $jam_skrg = date("H:i:s");
            $data['tgl'] = $tgl_skrg;
            $data['jam'] = $jam_skrg;
            $th = $this->uri->segment(3); 
            $id = $this->uri->segment(4); 
            $data['detail_daftar']=$this->m_tup_spj->detail_daftar($id,$th); 
            $detail_spm = $this->m_tup_spj->detail_daftar($id,$th);
                foreach($detail_spm->result_array() as $row)
                    {                  
                    $nomor=$row['no_spm']; 
                    $tahun=$row['th_spm']; 
                    $kode_digit=$row['kode_digit']; 
                    $singkat=$row['singkat']; 
                    //$jumlah=$row['jumlah']; 
                    }
                  $data['th'] = '20'.$tahun;
                  $data['singkat'] = $singkat;
                  $data['kode_digit'] = $kode_digit;
                  $data['nomor'] = sprintf("%04d", $nomor);      
            $data['cek_umum']=$this->m_tup_spj->ceklist_umum($id,$th);
            $data['cek_khusus']=$this->m_tup_spj->ceklist_khusus($id,$th);
            $data['setting']=$this->m_tup_spj->select_setting();   
            $this->template->load('template','tup_spj/data_terceklist2',$data);  
        }}
    function revisi_kelengkapan_penerima(){  
            $th = $this->uri->segment(3); 
            $id = $this->uri->segment(4);        
            
            //echo "<meta http-equiv='refresh' content='0; url=".site_url()."unit/list_spm_up_pen'>"; 
            $sudah = $this->m_tup_spj->sudah_cek_kelengkapan($id,$th);
            if($sudah){  
                    $success = $this->m_tup_spj->ada_flag_cek($id,$th); 
                    if($success){
                            $this->m_tup_spj->revisi_spm_kelengkapan($id,$th);
                            $this->session->set_userdata('msg_typ','ok');
                            $this->session->set_userdata('msg', 'SPM berhasil dikembalikan ke unit');
                            redirect('tup_spj/list_spm_gu_pen');  
                    }else{
                            $this->session->set_userdata('msg_typ','err');
                            $this->session->set_userdata('msg', 'kelengkapan sudah fix, tidak perlu direvisi');
                            redirect('tup_spj/list_spm_gu_pen');  
                    }
            }else{
                    $this->session->set_userdata('msg_typ','err');
                    $this->session->set_userdata('msg', 'SPM ini belum dicek kelengkapan-nya.. mohon dicek terlebih dahulu!!!');
                    redirect('tup_spj/veri/'.$th."/".$id); 
            }
        }
