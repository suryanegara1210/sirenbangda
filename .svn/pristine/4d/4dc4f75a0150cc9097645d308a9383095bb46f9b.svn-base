            $id_unit = $this->uri->segment(5);
            $tgl_skrg = date("Y-m-d");
            $jam_skrg = date("H:i:s");
            $data['tgl'] = $tgl_skrg;
            $data['jam'] = $jam_skrg;            
            $data['carabayar'] = $this->m_tup_spj->get_carabayar();     
            $data['detail_spm'] = $this->m_tup_spj->detail_spm($id_unit,$id_spm,$th_spm);
            //echo $this->db->last_query();  
            $detail_spm = $this->m_tup_spj->detail_spm($id_unit,$id_spm,$th_spm); 
                    foreach($detail_spm->result_array() as $row)
                    {                  
                    $nomor=$row['no_spm']; 
                    $tahun=$row['th_spm']; 
                    $kode_digit=$row['kode_digit']; 
                    $singkat=$row['singkat']; 
                    $jumlah=$row['jumlah']; 
                    }
                  $data['tahun'] = '20'.$tahun;
                  $data['singkat'] = $singkat;
                  $data['kode_digit'] = $kode_digit;
                  $data['nomor'] = sprintf("%04d", $nomor); 
            $data['terbilang'] = $this->konversi->terbilang($jumlah) ;
            $data['total_pagu'] = $this->m_tup_spj->jum($id_unit,$id_spm,$th_spm);   
            $total_spp_kini = $this->m_tup_spj->jum_kini_spm($id_unit, $id_spm,$th_spm);  
            $spp_kini=array();
            foreach($total_spp_kini as $uraian_kini){               
                $spp_kini=array_merge($spp_kini,array($uraian_kini->kdoutput.$uraian_kini->kdkomponen.$uraian_kini->kdakun => $uraian_kini->jumlah));
             }
            
            $data['spp_kini']=$spp_kini;  

            
            
            $total_spp_diajukan=$this->m_tup_spj->total_spm_diajukan($id_unit,$id_spm,$th_spm); 
                        foreach($total_spp_diajukan->result_array() as $tot_spp_diajukan)
                             {  
                                $jum_spp_diajukan = $tot_spp_diajukan ['jumlah'];
                             }
            $data['jum_spp_diajukan'] =  $jum_spp_diajukan; 
            $this->template->load('template','tup_spj/preview_spm_pusat',$data);    
   }
   function update_status_spj(){
            $tahun = date('y');
            $data['data_spj_sp2d'] = $this->m_tup_spj->ambil_id_sp2d_spj($tahun);
            foreach($data['data_spj_sp2d']->result_array() as $row)
                    {                  
                    $id_sp2d=$row['id_sp2d'];
                    }
           $cek_kurang = $this->m_tup_spj->cek_status_spm_spj($id_sp2d,$tahun); 
           //echo $this->db->last_query();
           if($cek_kurang > 0){
                $this->session->set_userdata('msg_typ','err');
                $this->session->set_userdata('msg', 'Maaf masih ada SPM yang belum diotorisasi PR2');
                redirect('tup_spj/list_spm_up_sp2d');
           }else{
                $this->m_tup_spj->ubah_status_spj_sp2d($id_sp2d,$tahun);
                $this->session->set_userdata('msg_typ','ok');
                $this->session->set_userdata('msg', 'SP2D berhasil Di Update');
                redirect('tup_spj/list_spm_up_sp2d');
           }       
    }


}
