/**
*
FUNGSI INI DIGUNAKAN UNTUK membuat kwitansi gu baru
*/
   function buat_gup(){
        if($this->auth->is_logged_in() == true){  
          $ta = $this->m_settings->get_tahun_anggaran();
          $ta2 = $this->m_settings->get_tahun_anggaran2();
          $id_subunit = $this->session->userdata('id_subunit');
          
          $sp2d_belum_spj = $this->m_tup_spj->ada_sp2d_spj($id_subunit,$ta2);
          //echo $this->db->last_query(); 
          if($sp2d_belum_spj > 0){
                 $jum_paguMP=$this->m_tup_spj->jum_paguMP();
                 $jum_summKwitansi=$this->m_tup_spj->jum_summKwitansi();
                 $sisa_mp=$jum_paguMP-$jum_summKwitansi;
          
                $cek_sp2d_up=$this->m_tup_spj->cek_sp2d_up();
                $cek_ketersediaan_mp=$this->m_tup_spj->cek_ketersediaan_mp();
          //echo $this->db->last_query();
          
          $nilai_up=$this->m_tup_spj->nilai_up();
          $pagu_real=$this->m_tup_spj->pagu_real();
          $max_up1=$this->m_tup_spj->jumlah_tup_spj($id_subunit,$ta2);
          //echo $this->db->last_query(); 
          foreach($max_up1->result_array() as $row){
                 $max_up2 = $row['nilai_max'];
                 $id_sp2d = $row['id_sp2d'];
          }
          $data['id_sp2d'] = $id_sp2d;
          if(isset($max_up2))
             {$max_up=$max_up2;}
            else
            {$max_up=0;}
          //echo $this->db->last_query();
          //print_r($maks);
          //echo"$max_up";
          //$max_up=100000000;
          
          $selisih_pagu_up=$pagu_real-$nilai_up;
          $pembanding='';
          $nama_pembanding='';
/*          echo 'jum_paguMP= ' .$jum_paguMP;
          echo ', jum_summKwitansi= '.$jum_summKwitansi;
          echo ', sisa_mp = '.$sisa_mp;
          echo ', cek_sp2d_up = '.$cek_sp2d_up;
          echo ', cek_ketersediaan_mp = '.$cek_ketersediaan_mp;
*/          
            $kode_unit = $this->session->userdata('kode_subunit');
            $tahun_sekarang= date("Y");
            $data['jns_pajak'] = $this->m_tup_spj->get_jenis_pajak();
            $data['get_nama_unit'] = $this->m_tup_spj->get_nama_unit();
            $data['get_nama_subunit'] = $this->m_tup_spj->get_nm_subunit();
            $data['golongan'] = $this->m_tup_spj->getGolongan();
            $data['get_sum_up'] = $this->m_tup_spj->get_sum_up();
            $data['jum_paguMP']=$this->m_tup_spj->jum_paguMP();
            $data['jum_summKwitansi']=$this->m_tup_spj->jum_summKwitansi();
            $data['max_up']=$max_up;
            
          
          
          if($cek_sp2d_up<=0)
          {
              $this->session->set_userdata('msg_typ','err');
              $this->session->set_userdata('msg', 'Maaf anda belum mengajukan UP (SP2D UP), Kwitansi belum dapat dibuat');
              redirect('');                 
          }
          else if($cek_sp2d_up>0)
          {
              if($cek_ketersediaan_mp<=0)
              {
                  if($max_up<$selisih_pagu_up)
                  {$pembanding=$max_up;
                  $nama_pembanding='Maksimum TUP yang telah diajukan';}
                  else
                  {$pembanding=$selisih_pagu_up;
                  $nama_pembanding='Sisa Pagu Real - Nilai UP';}
                  $data['pembanding']=$pembanding;
                  $data['nama_pembanding']=$nama_pembanding;
                  $this->template->load('template','tup_spj/gu_buat_kuitansi',$data);  
               }
               else if($cek_ketersediaan_mp>0)
                {
                    if($sisa_mp>0)
                    {
                        if($max_up<$sisa_mp)
                        {$pembanding=$max_up;
                        $nama_pembanding='Max UP';}
                        else
                        {$pembanding=$jum_paguMP;
                        $nama_pembanding='Sisa MP';}
                        $data['nama_pembanding']=$nama_pembanding;
                        $data['pembanding']=$pembanding;
                          $this->template->load('template','tup_spj/gu_buat_kuitansi',$data);  
                    }
                    else if ($sisa_mp<=0)
                    {
                        $data['pembanding']=$max_up;
                        $data['nama_pembanding']='Max UP';
                          $this->template->load('template','tup_spj/gu_buat_kuitansi',$data);  
                    }
                }
          }} else{
              $this->session->set_userdata('msg_typ','err');
              $this->session->set_userdata('msg', 'Maaf Subunit anda belum mengajukan SP2D TUP!!');
              redirect(base_url());
          }
          
          
          
               
          
        }
    }

